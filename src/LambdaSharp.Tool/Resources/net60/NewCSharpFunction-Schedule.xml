<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>%%FRAMEWORK%%</TargetFramework>
    <Deterministic>true</Deterministic>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <RootNamespace>%%ROOTNAMESPACE%%</RootNamespace>
    <NoWarn>CS1998</NoWarn>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Condition="'$(LAMBDASHARP)'==''" Include="LambdaSharp.Schedule" Version="%%LAMBDASHARP_VERSION%%"/>
    <ProjectReference Condition="'$(LAMBDASHARP)'!=''" Include="$(LAMBDASHARP)\src\LambdaSharp.Schedule\LambdaSharp.Schedule.csproj" />
  </ItemGroup>
</Project>