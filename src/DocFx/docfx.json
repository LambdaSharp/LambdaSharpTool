{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "LambdaSharp/LambdaSharp.csproj"
          ],
          "cwd": ".."
        }
      ],
      "dest": "sdk",
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml"
        ]
      },
      {
        "files": [
          "modules/**.md",
          "modules/**/toc.yml"
        ]
      },
      {
        "files": [
          "syntax/**.md",
          "syntax/**/toc.yml"
        ]
      },
      {
        "files": [
          "sdk/**.yml",
          "sdk/index.md"
        ]
      },
      {
        "files": [
          "cli/**/toc.yml",
          "cli/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../../../Docs-LambdaSharpTool/",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "images/LambdaSharpLogo.svg",
      "_appTitle": "Î»# - Serverless .NET for AWS",
      "_disableContribution": true,
      "_enableNewTab": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}