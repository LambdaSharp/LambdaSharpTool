  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml"
        ]
      },
      {
        "files": [
          "modules/**.md",
          "modules/**/toc.yml"
        ]
      },
      {
        "files": [
          "syntax/**.md",
          "syntax/**/toc.yml"
        ]
      },
      {
        "files": [
          "sdk/*/**.yml",
          "sdk/toc.yml",
          "sdk/index.md"
        ]
      },
      {
        "files": [
          "cli/**/toc.yml",
          "cli/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "robots.txt"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../../Docs-LambdaSharpTool/",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "images/LambdaSharpLogo.svg",
      "_appTitle": "LambdaSharp - Serverless .NET for AWS",
      "_disableContribution": true,
      "_enableNewTab": true,
      "_description": "Build serverless applications with .NET on AWS",
      "_keywords": "lambdasharp, lambda sharp, aws, serverless, .net, dotnet, .net core, dotnetcore, c#, c sharp",
      "_baseUrl": "https://lambdasharp.net/"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap":{
      "baseUrl": "https://lambdasharp.net/",
      "priority": 0.5,
      "changefreq": "monthly",
      "fileOptions": {
        "**/index.md": {
          "priority": 0.8
        },
        "sdk/**.yml": {
          "priority": 0.3
        }
      }
    }
  }
