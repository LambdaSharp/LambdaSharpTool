<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Deterministic>true</Deterministic>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <RootNamespace>Sample.KinesisFirehose.FirehoseAnalyzerFunction</RootNamespace>
    <Nullable>enable</Nullable>
    <NoWarn>CS1998</NoWarn>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Amazon.Lambda.KinesisFirehoseEvents" Version="2.1.0"/>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="$(LAMBDASHARP)\src\LambdaSharp\LambdaSharp.csproj"/>
  </ItemGroup>
</Project>